{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "sql:default",
    "sql:allow-execute",
    "sql:allow-select",
    "sql:allow-close",
    "shell:allow-spawn",
    "shell:allow-stdin-write",
    "shell:allow-kill",
    "dialog:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://localhost:3847/*"
        },
        {
          "url": "http://127.0.0.1:3847/*"
        }
      ]
    }
  ]
}
